{
  "manifest_version": 2,

  "name": "Tab-Control",
  "description": "Helps users to manage their tabs... especially during exam season!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "./img/temp_icon.png",
    "default_popup": "popup.html",
    "default_title" : "Tab-Control"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}